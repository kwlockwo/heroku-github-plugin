{
  "name": "heroku-github-plugin",
  "description": "A Heroku CLI plugin for GitHub intergration",
  "version": "0.1.0",
  "author": "Keith Lockwood @kwlockwo",
  "bugs": "https://github.com/kwlockwo/heroku-github-plugin/issues",
  "dependencies": {
    "@heroku-cli/command": "^8.5.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "cli-ux": "^5.6.3",
    "got": "^11.8.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/kwlockwo/heroku-github-plugin",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kwlockwo/heroku-github-plugin",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
